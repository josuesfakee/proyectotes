<style type="text/css">

    .dataTables_filter {
        text-align: right;
    }

    tfoot {
        display: table-header-group;
    }

    #loading {
        width: 100%;
        height: 100%;
        top: 0px;
        left: 0px;
        position: fixed;
        display: block;
        z-index: 99;
        opacity: 0.7;
        background-color: #fff;
    }

    #loading-image {
        position: absolute;
        top: 40%;
        left: 45%;
        z-index: 100
    }

    /*ANIMATION ALERT*/
    .messagealert {
        color: red;
        position: relative;
        -webkit-animation: mymove 2s infinite; /* Safari 4.0 - 8.0 */
        animation: mymove 2s infinite;
    }

    .bordes {
        border-radius: 15px;
        border-style: solid;
        border-color: #00a8e0;
    }

    .espacio {
        padding-top: 5px;
    }

    /* Safari 4.0 - 8.0 */
    @@-webkit-keyframes mymove {
        from {
            font-size: 12px;
        }

        to {
            font-size: 13px;
        }
    }

    @@keyframes mymove {
        from {
            font-size: 12px;
        }

        to {
            font-size: 13px;
        }
    }
</style>
